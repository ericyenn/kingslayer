        screenGui:Destroy()
        screenGui = nil
        mainFrame = nil
        bossNameLabel = nil
        healthBarFrame = nil
        healthBarFill = nil
        multiplierLabel = nil
    end
    isVisible = false
    currentStack = 1
    healthStacks = 1
    print("BossHealthUI: Cleaned up")
end

-- Initialize
print("BossHealthUI: Authentic BotW-style module loaded")

return BossHealthUI ]]></string>
